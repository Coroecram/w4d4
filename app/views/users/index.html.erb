<% content_for :title  do%>
: All Users<% end %>

<h1> All Users </h1>

<% @users.each do |user| %>
  <strong><%= "LOGGED IN AS" if user.session_token == session[:session_token]%></strong>
  <b><%= user.email %></b><br>
  <%= user.to_json %><br>
  <a href="<%=user_url(user.id)%>">User Page</a> <hr>

<% end %>
